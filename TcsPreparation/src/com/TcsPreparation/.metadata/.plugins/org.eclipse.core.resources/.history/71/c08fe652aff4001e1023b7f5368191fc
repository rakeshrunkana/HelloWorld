package com.Triangle;

public class PyramidPattern {

	public static void main(String[] args) {
    for(int i =1; i<=5; i++) {
    	for(int j=1; j<=5; j++) {
    		for(int k=1;k<;k++) {
    			System.out.print("* ");
    		}
    		System.out.print(" ");
    	}
    	System.out.println(" ");
    }
	}

}
